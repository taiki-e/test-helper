{
  "arch": "sparc",
  "cpu": "v9",
  "crt-objects-fallback": "false",
  "crt-static-respected": true,
  "data-layout": "E-m:e-p:32:32-i64:64-i128:128-f128:64-n32-S64",
  "dynamic-linking": true,
  "env": "uclibc",
  "features": "+v8plus",
  "has-rpath": true,
  "has-thread-local": true,
  "late-link-args": {
    "gnu-cc": ["-mcpu=v9", "-m32"],
    "gnu-lld-cc": ["-mcpu=v9", "-m32"]
  },
  "linker-flavor": "gnu-cc",
  "llvm-target": "sparc-unknown-linux-uclibc",
  "max-atomic-width": 32,
  "os": "linux",
  "position-independent-executables": true,
  "relro-level": "full",
  "supported-split-debuginfo": ["packed", "unpacked", "off"],
  "target-endian": "big",
  "target-family": ["unix"],
  "target-pointer-width": 32
}
